{
  "name": "swimming-results-api",
  "version": "0.0.1",
  "description": "API for managing swimmers results to track progress",
  "main": "index.js",
  "scripts": {
    "build": "webpack-cli",
    "watch": "webpack-cli -w",
    "clean": "rimraf ./.aws_sam ./coverage ./.nyc_output",
    "clean:all": "rimraf ./.aws_sam ./coverage ./.nyc_output ./node_modules",
    "test": "mocha",
    "test:cov": "nyc mocha && open ./coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rigikulm/swimming-results-api.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "dynamodb",
    "typescript",
    "SAM"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rigikulm/swimming-results-api/issues"
  },
  "homepage": "https://github.com/rigikulm/swimming-results-api#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.75",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/sinon": "^9.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "aws-sam-webpack-plugin": "^0.9.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.12.0",
    "@aws-sdk/types": "^3.12.0",
    "@aws-sdk/util-dynamodb": "^3.12.0",
    "@greenhorn/faas-logger": "file:../faas-logger/dist"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "all": true
  }
}
